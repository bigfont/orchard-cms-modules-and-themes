<Placement>

  <Place Parts_Common_Metadata="-"/>
  <Place Parts_Common_Metadata_Summary="-"/>

  <Match ContentType="CouncilMeeting">
    <!--Dates-->
    <Place Fields_DateTime-StartDate="Content:0.0;Alternate=Fields_DateTime__Formatted" />
    <Place Fields_DateTime-EndDate="Content:0.1;Alternate=Fields_DateTime__Formatted" />
    <!--Address-->
    <Place Fields_Link-Link="Content:1.0" />
    <Place Fields_Input-StreetAddress="Content:1.1" />
    <Place Fields_Input-City="Content:1.2" />
    <Place Fields_Input-State="Content:1.3" />
    <Place Fields_Input-PostalCode="Content:1.4" />
    <Place Fields_Input-OtherLocationType="Content:1.5" />
    <!--Documents & Text-->
    <Place Fields_MediaLibraryPicker="Content:2.0" />
    <Place Fields_Common_Text="Content:2.1" />
  </Match>

  <Match ContentType="Spotlight" DisplayType="Detail">
    <Place Parts_Title="Content:0;Alternate=Parts_Title__h2" />
    <Place Fields_MediaLibraryPicker="Content:1;Alternate=Fields_MediaLibraryPicker__Small" />
    <Place Parts_Common_Body="Content:2" />
    <Place Fields_Link="Content:3" />
  </Match>

  <Match ContentType="WorkItem" DisplayType="Summary">
    <Place Parts_Title="Content:0;Alternate=Parts_Title__h2"/>
    <Place Parts_Common_Body="Content:3" />
    <Place Fields_MediaLibraryPicker="Content:2;Alternate=Fields_MediaLibraryPicker__Large" />
    <Place Fields_Link="Content:4" />
    <Place Parts_Title_Summary="-"/>
    <Place Parts_Common_Body_Summary="-" />
    <Place Fields_Common_Text="-" />
    <Place Fields_TaxonomyField="-" />
  </Match>

  <Match ContentType="WorkItem" DisplayType="Detail">
    <Place Parts_Title="Content:0"/>
    <Place Parts_Common_Body="Content:3" />
    <Place Fields_MediaLibraryPicker="Content:2;Alternate=Fields_MediaLibraryPicker__Large" />
    <Place Fields_Link="Content:4" />
    <Place Parts_Title_Summary="-"/>
    <Place Parts_Common_Body_Summary="-" />
    <Place Fields_Common_Text="-" />
    <Place Fields_TaxonomyField="-" />
  </Match>

  <Match ContentType="NewsItem" DisplayType="Summary">
    <Place Fields_MediaLibraryPicker="Content:0;Alternate=Fields_MediaLibraryPicker__Small" />
    <Place Fields_Input-ShortTitle="Content:1" />
    <Place Fields_Common_Text-Summary="Content:2" />
    <Place Parts_Common_Body="-" />
    <Place Fields_DateTime-Date="-" />
    <Place Parts_Title_Summary="-"/>
    <Place Parts_Common_Body_Summary="-" />
    <Place Fields_TaxonomyField="-" />
    <Place Parts_Title="-"/>
    <Place Fields_Link="-" />
  </Match>

  <Match ContentType="NewsItem" DisplayType="Detail">
    <Place Parts_Title="Content:0;Alternate=Parts_Title__h2"/>
    <Place Fields_DateTime-Date="Content:1;Alternate=Fields_DateTime__Formatted" />
    <Place Fields_MediaLibraryPicker="Content:2;Alternate=Fields_MediaLibraryPicker__Large" />
    <Place Parts_Common_Body="Content:3" />
    <Place Fields_Link="Content:4" />
    <Place Parts_Title_Summary="-"/>
    <Place Parts_Common_Body_Summary="-" />
    <Place Fields_Common_Text="-" />
    <Place Fields_TaxonomyField="-" />
    <Place Fields_Input-ShortTitle="-" />
  </Match>

  <Match ContentType="Event" DisplayType="Summary">
    <Place Parts_Title_Summary="Content:0;Alternate=Parts_Title_Summary__h3"/>
    <!--Dates-->
    <Place Fields_DateTime-StartDate="Content:0.0;Alternate=Fields_DateTime__Formatted" />
    <Place Fields_DateTime-EndDate="Content:0.1;Alternate=Fields_DateTime__Formatted" />
    <!-- Location -->
    <Place Fields_Input-City="-" />
    <Place Fields_Input-State="-" />
    <!--Other-->
    <Place Parts_Common_Body_Summary="-" />
    <Place Fields_Link="-" />
  </Match>

  <Match ContentType="Event" DisplayType="Detail">
    <Place Parts_Title_Summary="-"/>
    <!--Dates-->
    <Place Fields_DateTime-StartDate="Content:0.0;Alternate=Fields_DateTime__Formatted" />
    <Place Fields_DateTime-EndDate="Content:0.1;Alternate=Fields_DateTime__Formatted" />
    <!-- Location -->
    <Place Fields_Input-City="Content:0.2" />
    <Place Fields_Input-State="Content:0.3" />
    <!--Other-->
    <Place Parts_Common_Body_Summary="-" />
    <Place Fields_Link="-" />
  </Match>

  <Match ContentType="Document" Path="council-meeting/*">
    <Place Parts_Title_Summary="-" />
  </Match>

  <Match ContentType="Image" DisplayType="Summary">
    <Place Parts_Title_Summary="-"/>
  </Match>

</Placement>
